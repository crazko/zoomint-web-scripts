{
  "name": "@zoomint/web-scripts",
  "private": true,
  "version": "0.1.0",
  "description": "Common web scripts",
  "bin": {
    "web-scripts": "./bin/run"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.2",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "cosmiconfig": "^6.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.8.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "execa": "^3.4.0",
    "jest": "^24.9.0",
    "prettier": "1.19.1",
    "ts-jest": "^24.2.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@types/node": "^10.17.7",
    "copyfiles": "^2.1.1",
    "rimraf": "^3.0.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
  "files": [
    "/bin",
    "/lib",
    "/package-lock.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "web-scripts",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "audit": "node ./bin/run audit",
    "build": "tsc --build",
    "postbuild": "copyfiles --flat src/configs/* lib/configs",
    "clean": "rimraf lib test-results",
    "format": "node ./bin/run format",
    "lint": "node ./bin/run lint",
    "postpack": "rimraf oclif.manifest.json",
    "prepack": "npm run clean && npm run build && oclif-dev manifest",
    "test": "node ./bin/run test",
    "posttest": "npm run lint"
  }
}
