{
  "name": "@zoomint/web-scripts",
  "private": true,
  "version": "0.1.0",
  "description": "Common web scripts",
  "bin": {
    "web-scripts": "./bin/run"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.2",
    "@spotify/web-scripts-utils": "^3.0.0",
    "cosmiconfig": "^6.0.0",
    "eslint": "^5.16.0",
    "eslint-config-esnext": "^4.0.0",
    "eslint-config-prettier": "^6.8.0",
    "execa": "^3.4.0",
    "prettier": "^1.19.1",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/node": "^10.17.7",
    "copyfiles": "^2.1.1",
    "globby": "^10.0.1",
    "rimraf": "^3.0.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
  "files": [
    "/bin",
    "/lib",
    "/package-lock.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "web-scripts",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "assets": "copyfiles --flat src/configs/* lib/configs",
    "build": "tsc --build",
    "clean": "rimraf lib",
    "lint": "node ./bin/run lint",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "npm run clean && npm run build && npm run assets && oclif-dev manifest",
    "test": "echo NO TESTS"
  }
}
